+name: Unlock Production Gate
+
+on:
+  workflow_dispatch:
+  push:
+    branches: [main, release/*]
+
+env:
+  FEATURE_PROD_ENABLED: 0  # Must be 1 to pass
+
+jobs:
+  verify_unlock:
+    runs-on: ubuntu-latest
+    steps:
+      - uses: actions/checkout@v3
+
+      - name: Check Production Flag
+        run: |
+          if [ "$FEATURE_PROD_ENABLED" != "1" ]; then
+            echo "❌ FEATURE_PROD_ENABLED is not 1"
+            exit 1
+          fi
+
+      - name: Setup Python
+        uses: actions/setup-python@v4
+        with:
